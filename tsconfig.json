{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6"
      ],
      "target": "ES2016",
      "module": "CommonJS",
      "moduleResolution": "node",
      "outDir": "./build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "sourceMap": true,

      "forceConsistentCasingInFileNames": true, // distinción entre mayúsculas y minúsculas
   },
}